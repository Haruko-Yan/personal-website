</main>
<footer>
  <div class="footer-container">
    <div class="running-time"></div>
  </div>
  </footer>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
  <script src="/js/layout.js"></script>
  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  <script>
    function updateDateTime() {
      const currentTime = new Date();
      let diff = currentTime.getTime() - "<%=startTime.getTime() %>";
      let year = Math.floor(diff / (24 * 3600 * 1000) / 365);
      let day = Math.floor((diff / 1000 - year * 365 * 24 * 3600) / (24 * 3600));
      let hour = Math.floor((diff / 1000 - year * 365 * 24 * 3600 - day * 24 * 3600) / 3600);
      let min = Math.floor((diff / 1000 - year * 365 * 24 * 3600 - day * 24 * 3600 - hour * 3600) / 60);
      let sec = Math.floor(diff / 1000 - year * 365 * 24 * 3600 - day * 24 * 3600 - hour * 3600 - min * 60);
      $(".running-time").text("This website has been running for " + year + " years " + day + " days " + hour + " hours " + min + " minutes " + sec + " seconds.")
    }
    setInterval(updateDateTime, 1000);
  </script>
</body>
</html>